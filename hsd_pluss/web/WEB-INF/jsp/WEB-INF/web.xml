<?xml version="1.0" encoding="UTF-8"?>
<web-app version="3.1" xmlns="http://xmlns.jcp.org/xml/ns/javaee" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://xmlns.jcp.org/xml/ns/javaee http://xmlns.jcp.org/xml/ns/javaee/web-app_3_1.xsd">
    <context-param>
        <param-name>contextConfigLocation</param-name>
        <param-value>/WEB-INF/applicationContext.xml</param-value>
    </context-param>
    <listener>
        <listener-class>org.springframework.web.context.ContextLoaderListener</listener-class>
    </listener>
    <servlet>
        <servlet-name>dispatcher</servlet-name>
        <servlet-class>org.springframework.web.servlet.DispatcherServlet</servlet-class>
        <load-on-startup>2</load-on-startup>
    </servlet>
    <servlet>
        <servlet-name>Registro</servlet-name>
        <servlet-class>Servlet.Registro</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>Iniciosesion</servlet-name>
        <servlet-class>Servlet.Iniciosesion</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>CiudadDAO</servlet-name>
        <servlet-class>ModeloDAO.CiudadDAO</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>Grosor_hiloDAO</servlet-name>
        <servlet-class>ModeloDAO.Grosor_hiloDAO</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>ProveedorDAO</servlet-name>
        <servlet-class>ModeloDAO.ProveedorDAO</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>RolesDAO</servlet-name>
        <servlet-class>ModeloDAO.RolesDAO</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>Tipo_documentoDAO</servlet-name>
        <servlet-class>ModeloDAO.Tipo_documentoDAO</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>UsuariosDAO</servlet-name>
        <servlet-class>ModeloDAO.UsuariosDAO</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>dispatcher</servlet-name>
        <url-pattern>*.htm</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>Registro</servlet-name>
        <url-pattern>/Registro</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>Iniciosesion</servlet-name>
        <url-pattern>/Iniciosesion</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>CiudadDAO</servlet-name>
        <url-pattern>/CiudadDAO</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>Grosor_hiloDAO</servlet-name>
        <url-pattern>/Grosor_hiloDAO</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>ProveedorDAO</servlet-name>
        <url-pattern>/ProveedorDAO</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>RolesDAO</servlet-name>
        <url-pattern>/RolesDAO</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>Tipo_documentoDAO</servlet-name>
        <url-pattern>/Tipo_documentoDAO</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>UsuariosDAO</servlet-name>
        <url-pattern>/UsuariosDAO</url-pattern>
    </servlet-mapping>
    <session-config>
        <session-timeout>
            30
        </session-timeout>
    </session-config>
    <welcome-file-list>
        <welcome-file>redirect.jsp</welcome-file>
    </welcome-file-list>
</web-app>
